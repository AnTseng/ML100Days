    "print(type(a))\n",
    "\n",
    "print(a.ndim) #陣列有多少維度\n",
    "print(a.shape) #每個維度的大小\n",
    "print(a.size) #陣列當中有幾個元素\n",
    "print(a.dtype) #陣列中的資料型態\n",
    "print(a.itemsize) #陣列中每個元素佔用的空間\n",
